#include <main.h>

void main()
{
   
   set_tris_b(0x00);
   output_b(0x00);

   int arr[10] ={0x3F, 0x06, 0x5C, 0x4F, 0x66, 0x6E, 0x7E, 0x07, 0x7F, 0x6F};

   while(TRUE)
   {
       output_c(arr[0]);
       delay_ms(1000);
       output_c(0x00);
       
       output_c(arr[1]);
       delay_ms(1000);
       
        output_c(arr[2]);
       delay_ms(1000);
       
        output_c(arr[3]);
       delay_ms(1000);
       
        output_c(arr[4]);
       delay_ms(1000);
   }

}
