#include <main.h>

void main()
{
   set_tris_b(0x00);
   set_tris_a(0xff);
   
   int sayac;
   output_b(0x00);
   
   while(TRUE)
   {
      for(sayac == 0; sayac < 5; sayac++) {
         if(input(pin_A1) == 1){delay_us(200);}
            
         if(input(pin_A1) == 0) {delay_us(200);}
            
      }
      
      if(sayac == 5){output_high(pin_B4);}
         
      
   }

}
