#include <main.h>

int dutyCycle = 4;
void main()
{
   setup_timer_2(T2_DIV_BY_16,124,1);      //2,0 ms overflow, 2,0 ms interrupt

   setup_ccp2(CCP_PWM);
   set_pwm2_duty(0);

   while(TRUE)
   {
      if(input(pin_A0) == 1)
      {
         delay_ms(20);
         while(input(pin_A0) == 1);
         delay_ms(20);
      }
      
      if(input(pin_A1) == 1)
      {
         
      }
   }

}
