#include <main.h>

void main()
{
   set_tris_a(0x00);
   set_tris_a(0xff);
   output_b(0x00);
   
   while(TRUE) {
      if(input(pin_A1) == 1){
         if(inputs(pin_A1) == 0) {
            output_high(pin_b4);
         }
             
         
      }
   
      if(input(pin_A2) == 1) {
         output_low(pin_b4);   
      }
   }
   
         
   
         

}
